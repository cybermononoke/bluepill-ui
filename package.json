{
  "name": "@cybermononoke/bluepill-ui",
  "version": "0.0.0",
  "private": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    ".tamagui"
  ],
  "scripts": {
    "clean": "rimraf dist .tamagui",
    "build:types": "tsc -p tsconfig.build.json",
    "build:tamagui": "tamagui build",
    "build:bundle": "rollup -c",
    "build": "npm run clean && npm run build:tamagui && npm run build:types && npm run build:bundle",
    "prepare": "npm run build"
  },
  "dependencies": {
    "tamagui": "1.132.15",         
    "@tamagui/config": "1.132.15",
    "@tamagui/theme-builder": "1.132.15",
    "@tamagui/colors": "1.132.15"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.69"
  },
  "devDependencies": {
    "@tamagui/cli": "1.132.15",
    "typescript": "^5.0.0",
    "rollup": "^3.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "rimraf": "^3.0.2"
  }
}
