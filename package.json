{
  "name": "bluepill-ui",
  "version": "1.0.0",
  "description": "A Tamagui UI library with scoped configuration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
 "build": "tsc && npm run copy-assets",
    "copy-assets": "cp -r src/components/assets dist/src/components/ || xcopy src\\components\\assets dist\\src\\components\\assets\\ /E /I",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module"
  },
  "keywords": [
    "react-native",
    "expo",
    "tamagui",
    "ui"
  ],
  "repository": "https://github.com/cybermononoke/bluepill-ui",
  "bugs": {
    "url": "https://github.com/cybermononoke/bluepill-ui/issues"
  },
  "author": "cybermononoke",
  "license": "MIT",
  "homepage": "https://github.com/cybermononoke/bluepill-ui#readme",
  "dependencies": {
  },
"peerDependencies": {
  "expo": "*",
  "expo-font": "*",
  "react": "*",
  "react-native": "*",
  "tamagui": "1.132.15",
  "@tamagui/core": "1.132.15",
  "@tamagui/colors": "1.132.15",
  "@tamagui/config": "1.132.15",
  "@tamagui/animations-react-native": "1.132.18",
  "@tamagui/lucide-icons": "1.132.15",
  "@tamagui/shorthands": "1.132.15",
  "@tamagui/theme-builder": "1.132.15",
  "@tamagui/toast": "1.132.15"
},
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0",
    "expo-module-scripts": "^3.5.2"
  },
  "expo": {
    "name": "bluepill-ui",
    "slug": "bluepill-ui"
  }
}
